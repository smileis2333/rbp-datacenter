server:
  port: 8177

#设置数据库连接池的最大连接数扩容倍数，设置值为0.5-1.5  不在范围值则设置无效
datasourcesconfig:
  maxActiveDilatation: 1.5

spring:
  application:
    name: APP-SERVER
  druid:
    username: druid
    password: 123456
  datasource:
    # 使用druid数据源
    type: com.alibaba.druid.pool.DruidDataSource
    initialSize: 5
    minIdle: 5
    maxActive: 50
    maxWait: 60000
    timeBetweenEvictionRunsMillis: 60000
    minEvictableIdleTimeMillis: 25200000
    testWhileIdle: true
    testOnBorrow: false
    testOnReturn: false
    poolPreparedStatements: true
    maxPoolPreparedStatementPerConnectionSize: 20
    filters: stat,slf4j
    logSlowSql: true
    connectionProperties: druid.stat.mergeSql=true;druid.stat.slowSqlMillis=5000

    #    # 主库连接设置
    master:
      dbname: rbpMaster
      driverClassName: com.mysql.cj.jdbc.Driver
      url: jdbc:mysql://119.29.107.244:20001/rbp?useUnicode=true&characterEncoding=utf-8&useJDBCCompliantTimezoneShift=true&useLegacyDatetimeCode=false&allowMultiQueries=true&allowMultiQueries=true&serverTimezone=GMT%2B8
      username: root
      password: root
      validationQuery: SELECT 1

    # 从库连接设置
    slave1:
      dbname: rbpSlave1
      driverClassName: com.mysql.cj.jdbc.Driver
      url: jdbc:mysql://119.29.107.244:20002/rbp?useUnicode=true&characterEncoding=utf-8&useJDBCCompliantTimezoneShift=true&useLegacyDatetimeCode=false&allowMultiQueries=true&allowMultiQueries=true&serverTimezone=GMT%2B8
      username: root
      password: root
      validationQuery: SELECT 1

  # 时间格式化
  jackson:
    date-format: yyyy-MM-dd
    time-zone: GMT+8
    default-property-inclusion: always

# MyBatis
mybatis-plus:
  # 搜索指定包别名
  typeAliasesPackage: com.regent.rbp.**.model
  # 配置mapper的扫描，找到所有的mapper.xml映射文件
  mapperLocations: classpath*:mappers/**/*Mapper.xml
  # 原生配置
  configuration:
    map-underscore-to-camel-case: true
    cache-enabled: true
    #打印sql
    log-impl: org.apache.ibatis.logging.stdout.StdOutImpl
---
# Admin监控管理设置
spring:
  security:
    user:
      name: "admin"
      password: "admin"

eureka:
  client:
    registryFetchIntervalSeconds: 5
    service-url:
      defaultZone: http://119.29.107.244:8001/eureka/
  instance:
    leaseRenewalIntervalInSeconds: 10
    health-check-url-path: /actuator/health
    metadata-map:
      user.name: ${spring.security.user.name}
      user.password: ${spring.security.user.password}

management:
  endpoints:
    web:
      exposure:
        include: "*"
  endpoint:
    health:
      show-details: ALWAYS

---
# Redis数据库设置
spring:
  redis:
    database: 2
    host: 119.29.107.244
    port: 7001
    password: Regentsoft2020
    timeout: 10000
    pool:
      max-active: 200
      max-wait: -1
      max-idle: 10
      min-idle: 0

